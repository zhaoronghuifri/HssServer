<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="classes">

	<class name="CallLog" table="CALLLOG">

		<id name="callid" column="CALLID">
			<generator class="assigned" />
		</id>

		<property name="status" column="STATUS" />
		<property name="caller" column="CALLER" />
		<property name="callee" column="CALLEE" />
		<property name="begain" column="BEGAIN" />
		<property name="end"    column="END" />		
		<property name="phrase" column="PHRASE" />
		<property name="total"  column="TOTAL" />
		<property name="flow"   column="FLOW" />
		


	</class>

</hibernate-mapping>  
