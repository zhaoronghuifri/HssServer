<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from KeyWSService WSDL service -->

  <service name="KeyWS" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws.fri.org"/>
      <parameter name="wsdlServiceElement" value="KeyWSService"/>
      <parameter name="schemaQualified" value="http://ws.fri.org"/>
      <parameter name="wsdlServicePort" value="KeyWS"/>
      <parameter name="className" value="org.fri.ws.KeyWSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="KeyWS"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getTek" qname="operNS:getTek" xmlns:operNS="http://ws.fri.org" returnQName="retNS:getTekReturn" xmlns:retNS="http://ws.fri.org" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:callerid" xmlns:pns="http://ws.fri.org" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:calleeid" xmlns:pns="http://ws.fri.org" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:callid" xmlns:pns="http://ws.fri.org" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getTek"/>

  </service>
</deployment>
